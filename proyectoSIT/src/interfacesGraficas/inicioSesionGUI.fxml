<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controladores.InicioSesionController">
    <stylesheets>
        <URL value="@/css/inicioSesion.css" />
    </stylesheets>
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <bottom>
            <Pane id="barraInferior" prefHeight="25.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </bottom>
         <left>
            <Pane prefHeight="200.0" prefWidth="250.0" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fitHeight="225.0" layoutX="30.0" layoutY="75.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../imagenes/Logo%20SIT.png" />
                     </image>
                  </ImageView>
               </children></Pane>
         </left>
         <center>
            <FlowPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Label id="lbInicioSesion" prefHeight="25.0" prefWidth="300.0" text="%lbInicioSesion">
                     <FlowPane.margin>
                        <Insets left="25.0" top="75.0" />
                     </FlowPane.margin>
                  </Label>
                  <Label prefHeight="25.0" prefWidth="300.0" styleClass="lbInicioSesion" text="%lbNombreUsuario">
                     <FlowPane.margin>
                        <Insets left="25.0" top="20.0" />
                     </FlowPane.margin>
                  </Label>
                  <TextField prefWidth="200.0">
                     <FlowPane.margin>
                        <Insets left="25.0" />
                     </FlowPane.margin>
                  </TextField>
                  <Label prefHeight="25.0" prefWidth="300.0" styleClass="lbInicioSesion" text="%lbPassword">
                     <FlowPane.margin>
                        <Insets left="25.0" top="20.0" />
                     </FlowPane.margin>
                  </Label>
                  <TextField prefWidth="200.0">
                     <FlowPane.margin>
                        <Insets left="25.0" />
                     </FlowPane.margin>
                  </TextField>
                  <Button id="btnIngresar" mnemonicParsing="false" text="%btnIngresar">
                     <FlowPane.margin>
                        <Insets left="75.0" top="30.0" />
                     </FlowPane.margin>
                  </Button>
               </children>
            </FlowPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
